faro.receiver "integrations_app_agent_receiver" {
	extra_log_labels = {}

	server {
		listen_address           = "localhost"
		listen_port              = 55678
		max_allowed_payload_size = "4MiB786KiB832B"

		rate_limiting {
			enabled    = true
			rate       = 100
			burst_size = 50
		}
	}

	sourcemaps {
		download_from_origins = ["*"]
		download_timeout      = "1s"
	}

	output {
		logs   = [loki.write.logs_log_config.receiver]
		traces = []
	}
}

