prometheus.remote_write "test1" {
	endpoint {
		name = "test1-661de2"
		url  = "http://localhost:9009/api/prom/push"

		queue_config {
			capacity             = 2500
			max_shards           = 200
			max_samples_per_send = 500
		}

		metadata_config {
			max_samples_per_send = 500
		}
	}

	wal {
		truncate_frequency = "1h0m0s"
		max_keepalive_time = "4h0m0s"
	}
}

