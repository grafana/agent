discovery.azure "fun" {
	subscription_id = "subscription"

	oauth {
		client_id     = "client"
		tenant_id     = "tenant"
		client_secret = "secret"
	}

	managed_identity {
		client_id = "client"
	}
}

local.file_match "fun" {
	path_targets = discovery.azure.fun.targets
}

loki.source.file "fun" {
	targets    = local.file_match.fun.targets
	forward_to = []
}
